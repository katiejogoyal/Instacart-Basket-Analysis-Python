   "source": [
    "# 3 different ways of reaching a result of 100 by adding or subtracting numeric variables\n",
    "a = 50 + 50\n",
    "b = 200 - 100\n",
    "c = 25 * 4\n",
    "\n",
    "print(\"Three different ways to reach 100:\")\n",
    "print(f\"50 + 50 = {a}\")\n",
    "print(f\"200 - 100 = {b}\")\n",
    "print(f\"25 * 4 = {c}\")\n",
    "\n",
    "# Code 2 floating-point variables and divide them by each other\n",
    "float1 = 5.5\n",
    "float2 = 2.2\n",
    "result = float1 / float2\n",
    "\n",
    "print(\"\\nDivision of two floating-point variables:\")\n",
    "print(f\"{float1} / {float2} = {result}\")\n",
    "\n",
    "# Construct a short word made of separate strings (concatenation)\n",
    "part1 = \"Py\"\n",
    "part2 = \"thon\"\n",
    "word = part1 + part2\n",
    "\n",
    "print(\"\\nConcatenation to form a word:\")\n",
    "print(f\"{part1} + {part2} = {word}\")\n",
    "\n",
    "# Construct 2 short sentences made of separate strings\n",
    "str1 = \"Jupyter\"\n",
    "str2 = \"notebook\"\n",
    "sentence1 = str1 + \" \" + str2\n",
    "\n",
    "str3 = \"is\"\n",
    "str4 = \"awesome\"\n",
    "sentence2 = str3 + \" \" + str4\n",
    "\n",
    "print(\"\\nConcatenation to form sentences:\")\n",
    "print(f\"'{str1} {str2}'\")\n",
    "print(f\"'{str3} {str4}'\")\n"
